{
  "name": "@grandgold/utils",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @grandgold/types",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external @grandgold/types",
    "clean": "rm -rf dist node_modules",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@grandgold/types": "workspace:*"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.4",
    "speakeasy": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "vitest": "^1.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/speakeasy": "^2.0.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
